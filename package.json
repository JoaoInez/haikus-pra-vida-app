{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "yarn gentypes && expo start",
    "android": "yarn gentypes && expo start --android",
    "ios": "yarn gentypes && expo start --ios",
    "web": "yarn gentypes && expo start --web",
    "test": "jest -u",
    "test:dev": "jest -u -o --watch --coverage=false",
    "eject": "expo eject",
    "gentypes": "apollo client:codegen --addTypename --endpoint=http://localhost:4000/graphql --globalTypesFile=src/types/graphqlGlobalTypesFile.ts --includes=src/**/*.tsx --outputFlat --tagName=gql --target=typescript src/types",
    "publish": "expo publish"
  },
  "dependencies": {
    "@apollo/client": "^3.2.0",
    "dotenv": "^8.2.0",
    "expo": "~38.0.8",
    "expo-constants": "~9.1.1",
    "expo-font": "~8.2.1",
    "expo-status-bar": "^1.0.2",
    "graphql": "^15.3.0",
    "react": "~16.11.0",
    "react-dom": "~16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-appearance": "~0.3.3",
    "react-native-web": "~0.11.7"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-native": "^3.4.2",
    "@testing-library/react-native": "^7.0.2",
    "@types/jest": "^26.0.14",
    "@types/react": "~16.9.41",
    "@types/react-native": "~0.62.13",
    "apollo": "^2.30.3",
    "babel-jest": "^26.3.0",
    "jest-expo": "^38.0.2",
    "react-test-renderer": "^16.13.1",
    "typescript": "~3.9.5"
  },
  "private": true
}
